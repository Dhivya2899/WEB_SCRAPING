# Parse the HTML content
soup = BeautifulSoup(html, 'html.parser')

# Find the table with financial data
table = soup.find('div', class_='W(100%) Whs(nw) Ovx(a) BdT Bdtc($seperatorColor) tablescraper-hover')
# Find all rows within the table
rows = table.find_all('div', class_='tablescraper-selected-row')

# Open a CSV file in write mode
with open('financial_data.csv', 'w', newline='') as csvfile:
    # Create a CSV writer object
    csv_writer = csv.writer(csvfile)
    
    # Write the header row
    csv_writer.writerow(['Category', 'TTM', '12/30/2022', '12/30/2021', '12/30/2020'])
    
    # Iterate through each row and extract data
    for row in rows:
        # Extract the breakdown category
        category = row.find('div', class_='D(tbc)').text.strip()
        
        # Extract the financial data for TTM, 12/30/2022, 12/30/2021, and 12/30/2020
        data = [cell.text.strip() for cell in row.find_all('div', class_='D(tbc)')[1:]]
        
        # Write the row into the CSV file
        csv_writer.writerow([category] + data)

print("Data has been saved to financial_data.csv")

# METHOD 2
import requests
from bs4 import BeautifulSoup

# URL of the page to scrape
url = "https://finance.yahoo.com/quote/GOEV/financials"

# Fetch the HTML content
response = requests.get(url)
html = response.text

# Parse the HTML content
soup = BeautifulSoup(html, 'html.parser')

# Find the section containing financial data
section = soup.find('section', class_='smartphone_Px(20px) Mb(30px)')

# Find the table with financial data
table = section.find('div', class_='W(100%) Whs(nw) Ovx(a) BdT Bdtc($seperatorColor) tablescraper-hover')

# Find all rows within the table
rows = table.find_all('div', class_='tablescraper-selected-row')

# Iterate through each row and extract data
for row in rows:
    # Extract the breakdown category
    category = row.find('div', class_='D(tbc)').text.strip()
    
    # Extract the financial data for TTM, 12/30/2022, 12/30/2021, and 12/30/2020
    data = [cell.text.strip() for cell in row.find_all('div', class_='D(tbc)')[1:]]
    
    print(category, data) 
